@page
@model webserviceModel
@using System.Net.Http
@using Microsoft.Extensions.DependencyInjection;

@inject HttpClient httpClient

<h1>API Data Results for Brewery</h1>

<table>
    <thead>
        <tr>
        @foreach (var data in webserviceModel.DBEntry.headers)
        {
            <th>@data</th>
        }
        </tr>
    </thead>
    <tbody>
        @foreach (webserviceModel.DBEntry data in Model.webservice)
        {
            <tr>
                <td>@data.name</td>
                <td>@data.address</td>
                <td>@data.city</td>
                <td>@data.state</td>
                <td>@data.established</td>
                <td>@data.website</td>
                <td>@data.priceLevel</td>
            </tr>
        }
    </tbody>
</table>
